{
	"basePath": "./",
	"name": "Strata Panorama Types",
	"entryPoints": ["shared/**/*.d.ts", "p2ce/**/*.d.ts"],
	"customCss": "docs.css",
	"searchInComments": true,
	"excludeInternal": true,
	"kindSortOrder": [
		"Module",
		"Namespace",
		"Function",
		"Class",
		"Interface",
		"Property",
		"TypeAlias",
		"TypeLiteral"
	],
	"searchGroupBoosts": {
		"api": 30,
		"enum": 20
	},
	"navigationLinks": {
		"Home": "https://stratasource.org/",
		"GitHub": "https://github.com/StrataSource/",
	},
	"out": "./docs"
}